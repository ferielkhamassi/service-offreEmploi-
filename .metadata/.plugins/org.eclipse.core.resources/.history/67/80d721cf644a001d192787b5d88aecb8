package com.iset.entities;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;

	@Entity
	public class Offre { 
	@Id 
	@GeneratedValue
	long code; 
	 String intitulé; 
	 String specialité; 
	 String société; 
	int nbpostes; 
	 String pays;
	 
	public Offre() {
		super();
	}
	 
	 
} 